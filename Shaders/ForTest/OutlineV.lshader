#version 330 core
layout (location = 0) in vec3 pos; 
layout (location = 1) in vec2 textPos;
layout (location = 2) in vec3 normal;

struct MatrixMVP
{
	mat4 model;
	mat4 view;
	mat4 projection;
};
uniform MatrixMVP Matrix;


void main()
{
	gl_Position = Matrix.projection * Matrix.view * Matrix.model * vec4(pos.x,pos.y,pos.z,1);
}
